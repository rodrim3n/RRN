<table>
  <th>
    <td>Username</td>
    <td>Actions</td>
  </th>
  % admins.each do |admin|
  <tr>
    <td> {{admin.username}} </td>
    <td>
      <form method='post' action='/admin/delete/{{admin.id}}'>
        {{ app.csrf.form_tag }}
        <input type="hidden" name="_method" value="delete">
        <button type=submit>Delete</button>
      </form>
      <a href="/admin/edit/{{admin.id}}">Edit</a>
    </td>
  </tr>
  % end
<table>
<a href="/admin">Back</a>
